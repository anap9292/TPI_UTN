@model List<Empleado>
<h1>Vista de listar funciona?</h1>

<a asp-action="Guardar" asp-controller="Empleado" class="btn btn-primary">Crear Empleado</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">ID SUpervisor</th>
            <th scope="col">Usuario empleado</th>
         
        </tr>
    </thead>





    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>@item.empl_id</td>
                <td>@item.empl_nombre</td>
                <td>@item.empl_apellido</td>
                <td>@item.empl_supervisor_id</td>
                <td>@item.empl_user</td>

                <td><a asp-action="Editar" asp-controller="Empleado" class="btn btn-secondary" asp-route-id="@item.empl_id">Editar</a></td>
                <td><a asp-action="Eliminar" asp-controller="Empleado" class="btn btn-danger" asp-route-id="@item.empl_id">Eliminar</a></td>
            </tr>
        </tbody>
    }

</table>